<h3>List of Users</h3>

<section class="media-table">
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id)%></td>
    </tr>
    <% end %>
  </tbody>
</table>
</section>

<td><a class="btn btn-secondary" <%= link_to "Back to Media list", works_path %></a>
  </td>
